<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <Version>3.1.0</Version>
    <PackageProjectUrl>https://github.com/phongnguyend/Practical.Cryptography</PackageProjectUrl>
